#include <stdio.h>
#include <math.h>
int main() {
 
    double x1,x2,y1,y2,distancia,difx,dify;
    scanf("%lf %lf\n",&x1,&y1);
    scanf("%lf %lf",&x2,&y2);
    difx = (x2-x1)*(x2-x1);
    dify = (y2-y1)*(y2-y1);
    distancia = sqrt(difx +dify);
    printf("%.4lf\n",distancia);
     
 
    return 0;
}
