//par ou impar
#include <stdio.h>
int main()
{
    int n;//quantidade de valores lidos 
    int i=0;
    scanf("%d",&n); 
    int x;//valores que serao lidos
    for(i=0;i<=n-1;i++)
    { 
        scanf("%i",&x);
        if(x == 0)
        {
            printf("NULL\n"); 
        }
        else
        {
            if(x%2 == 0)
            {
                printf("EVEN");
            }
            else
            {
                printf("ODD");
            }
            if(x > 0)
            {
                printf(" POSITIVE\n");
                
            }
            else 
            {
                printf(" NEGATIVE\n"); 
            
            }
        }
    }    
     return 0; 
    
}
