//crescente e decrescente
#include <stdio.h>
int main() 
{
    int X=0,Y=1;//valores a serem avaliados 
    while(X!=Y)
    {
        scanf("%d %d",&X,&Y);
        if(X<Y)
        {
          printf("Crescente\n");
        }
        if(X>Y)
        {
          printf("Decrescente\n");
        }
    }
    return 0;
}
