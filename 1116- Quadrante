//quadrantes
#include <stdio.h>
int main() 
{
    int X=1,Y=1;//coordenadas no plano cartesiano
    while(X!=0 || Y!=0)
    {
        scanf("%d %d",&X,&Y);
        if(0<X && 0<Y)
        {
            printf("primeiro\n");
        }
        if(0<X && Y<0)
        {
            printf("quarto\n");
        }
        if(X<0 && 0<Y)
        {
            printf("segundo\n");
        }
        if(X<0 && Y<0)
        {
            printf("terceiro\n");
        }
    }
    return 0;
}
